{
  "name": "hyperfy-apps",
  "version": "1.0.0",
  "description": "Hyperfy V1 and V2 app archive with extraction tools",
  "type": "module",
  "scripts": {
"catalog:build": "uv run python scripts/catalog/build_catalog.py",
    "catalog:dry-run": "uv run python scripts/catalog/build_catalog.py --dry-run",
    "catalog:context": "uv run python scripts/research/prepare_context_bundle.py",
    "catalog:summarize": "uv run python scripts/research/summarize_hyp_files_openrouter.py",
    "catalog:summarize:dry-run": "uv run python scripts/research/summarize_hyp_files_openrouter.py --dry-run",
    "explorer:build": "uv run python scripts/catalog/build_explorer_data.py",
    "explorer:serve": "uv run python -m http.server 8080",
    "build:all": "uv run python scripts/catalog/build_catalog.py --skip-optimize && uv run python scripts/catalog/build_explorer_data.py"
  },
  "keywords": [
    "hyperfy",
    "virtual-worlds",
    "metaverse",
    "apps"
  ],
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
